<?xml version="1.0" encoding="utf-8"?>
<android.support.v7.widget.CardView
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/swipeReveal"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    app:cardUseCompatPadding="true">




    <LinearLayout
        android:id="@+id/linear_category"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

       <android.support.v7.widget.CardView
            android:layout_width="65dp"
            android:layout_height="wrap_content"
            android:layout_marginBottom="5.4dp"
            android:layout_marginLeft="13.4dp"
            android:layout_marginTop="5.4dp"
            app:cardCornerRadius="30dp"

           app:cardBackgroundColor="@color/colorPrimary"
            app:cardElevation="10dp">

            <ImageView
                android:id="@+id/circle_image"
                android:layout_width="65dp"
                android:layout_height="65dp"



                android:layout_marginRight="13.4dp"
                android:src="@drawable/foodimg2"
               />
    </android.support.v7.widget.CardView>

        <RelativeLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center"

            android:layout_weight="1">

            <TextView
                android:id="@+id/itemname1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="13.4dp"
                android:text="Category name"
                android:textColor="#262628"
                android:textSize="18sp"
                android:textStyle="bold"  />

            <ImageView
            android:layout_width="wrap_content"
                android:layout_height="wrap_content"
               android:layout_alignParentEnd="true"
                android:layout_marginEnd="7dp"

          app:srcCompat="@drawable/ic_right_black_arrow" />

</RelativeLayout>


